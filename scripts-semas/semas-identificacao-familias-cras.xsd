<?xml version="1.0" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns="http://schemas.fortaleza.ce.gov.br/acao/semas-identificacao-familias-cras.xsd"
    xmlns:class="http://schemas.fortaleza.ce.gov.br/acao/classificacao.xsd"
    xmlns:idx="http://schemas.fortaleza.ce.gov.br/acao/indexhint.xsd"
    xmlns:xsdext="http://schemas.fortaleza.ce.gov.br/xsdform/extensions.xsd"
    xmlns:xhtml="http://www.w3.org/1999/xhtml"
    targetNamespace="http://schemas.fortaleza.ce.gov.br/acao/semas-identificacao-familias-cras.xsd"
    elementFormDefault="qualified">

    <xs:element name="formFichaIdentificacaoFamilia" type="formFichaIdentificacaoFamilia">
        <xs:annotation>
            <xs:appinfo>
                <xhtml:label>Ficha de Identificação Família</xhtml:label>
                <class:classificacoes>
                    <class:classificacao>cn=Assistencia Social</class:classificacao>
                </class:classificacoes>
                <idx:index>
                    <idx:hint key="pessoa.numeroInscricao" xpath="formFichaIdentificacaoFamilia/identificacao/nuneroInscricao"/>
                    <idx:hint key="pessoa.numeroNis" xpath="formFichaIdentificacaoFamilia/identificacao/nuneroNis"/>
                </idx:index>
            </xs:appinfo>
        </xs:annotation>
    </xs:element>

	<xs:complexType name="formFichaIdentificacaoFamilia">
        <xs:sequence>

			<xs:element name="identificacao">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>Identificação</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				
				<xs:complexType>
					<xs:sequence>
				        <xs:element name="numeroInscricao" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Número Inscrição</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="numeroNis" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>NIS</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>


						<xs:element name="dataCadastro" minOccurs="0" type="xs:date">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Data Cadastro</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="regional"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Regional</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="SER I" />
								<xs:enumeration value="SER II" />
								<xs:enumeration value="SER III" />
								<xs:enumeration value="SER IV" />
								<xs:enumeration value="SER V" />
								<xs:enumeration value="SER VI" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="cras"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>CRAS</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="PIRAMBU" />
								<xs:enumeration value="BARRA DO CEARÁ" />
								<xs:enumeration value="MUCURIPE" />
								<xs:enumeration value="SERVILUZ" />
								<xs:enumeration value="LAGAMAR" />
								<xs:enumeration value="PRAIA DO FUTURO " />
								<xs:enumeration value="BELA VISTA" />
								<xs:enumeration value="QUINTINO CUNHA " />
								<xs:enumeration value="VILA UNIÃO" />
								<xs:enumeration value="SERRINHA" />
								<xs:enumeration value="COUTO FERNANDES" />
								<xs:enumeration value="GRANJA PORTUGAL" />
								<xs:enumeration value="BOM JARDIM" />
								<xs:enumeration value="MONDUBIM" />
								<xs:enumeration value="GENIBAÚ" />
								<xs:enumeration value="CONJUNTO ESPERANÇA" />
								<xs:enumeration value="CANINDEZINHO" />
								<xs:enumeration value="ARACAPÉ" />
								<xs:enumeration value="CONJUNTO PALMEIRAS" />
								<xs:enumeration value="JARDIM DAS OLIVEIRAS" />
								<xs:enumeration value="CASTELÃO" />
								<xs:enumeration value="JOÃO PAULO II" />
								<xs:enumeration value="DENDÊ" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="origemAtendimento"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Origem Atendimento</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Demanda Expontânea" />
								<xs:enumeration value="Busca Ativa" />
								<xs:enumeration value="Demanda Encaminhada" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="origemEncaminhamento"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Origem Encaminhamento</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Assistência Social" />
								<xs:enumeration value="Saúde" />
								<xs:enumeration value="Educação" />
								<xs:enumeration value="habitação" />
								<xs:enumeration value="Trabalho e Renda" />
								<xs:enumeration value="Direitos Humanos" />
								<xs:enumeration value="Segurança Pública" />
								<xs:enumeration value="Sistema de Garantia de Direito" />
								<xs:enumeration value="ONG" />
								<xs:enumeration value="Outros" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>
				        <xs:element name="origemEncaminhamentoOutros" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Outros</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

		            </xs:sequence>
		        </xs:complexType>
	        </xs:element>


			<xs:element name="identificacaoresponsavel">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>1. Identificação do Responsável</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				
				<xs:complexType>
					<xs:sequence>
				        <xs:element name="nomeResponsavel" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.1 Nome</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="naturalidade" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.2 Naturalidade</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="estadoCivil" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.3 Estado Civil</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="numRG" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.4 RG</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                    <xs:pattern value="[0-9]+[0-9]+"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="orgaoExpedidor" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Órgão Expedidor</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>


						<xs:element name="dataExpedicaoRG" minOccurs="0" type="xs:date">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Data Expedição</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
				        <xs:element name="numCPF" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.5 CPF</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                    <xs:pattern value="[0-9]{3}\."/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="endereco" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.6 Endereço</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="bairro" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Bairro</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="comunidadeLocalidade" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Comunidade/Localidade</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>


						<xs:element name="regionalResponsavel"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>SER</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="SER I" />
								<xs:enumeration value="SER II" />
								<xs:enumeration value="SER III" />
								<xs:enumeration value="SER IV" />
								<xs:enumeration value="SER V" />
								<xs:enumeration value="SER VI" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>
				        <xs:element name="pontoReferencia" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Ponto de Referencia</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>
				        
						<xs:element name="telefone"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.7 Telefone</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Próprio" />
								<xs:enumeration value="Comunitário" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>
				        <xs:element name="numeroTelefone" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Nº Telefone</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="recadoContato" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Recado/Contato</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>


						<xs:element name="sexo"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.8 Sexo</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Masculino" />
								<xs:enumeration value="Feminino" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="identidadeGenero"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.9 Identidade de gênero</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Travesti" />
								<xs:enumeration value="Transexual" />
								<xs:enumeration value="Trangênero" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="orientacaoSexual"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.10 Orientação Sexual</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Homossexualidade/Lesbianidade" />
								<xs:enumeration value="Bissexualidade" />
								<xs:enumeration value="Heterosexualidade" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="corRaca"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.11 Cor ou Raça</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Preta" />
								<xs:enumeration value="Branca" />
								<xs:enumeration value="Parda" />
								<xs:enumeration value="Amarela" />
								<xs:enumeration value="Indígena" />
								<xs:enumeration value="Sem declaração" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

		            </xs:sequence>
		        </xs:complexType>
	        </xs:element>

			<xs:element name="situacaomoradia">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>2. Situação de Moradia</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				
				<xs:complexType>
					<xs:sequence>

						<xs:element name="tipo"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Tipo</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Casa" />
								<xs:enumeration value="Apartamento" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>
				        <xs:element name="numeroComodos" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Número de Cômodos</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>



						<xs:element name="banheiro"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Banheiro</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Sim" />
								<xs:enumeration value="Não" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="afirmativoBanheiro"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Caso Afirmativo, encontra-se em</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="dentro do domicílio" />
								<xs:enumeration value="fora do domicílio" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="situacaoImovel"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Situação Imóvel</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Próprio" />
								<xs:enumeration value="Alugado(a)" />
								<xs:enumeration value="Cedida" />
								<xs:enumeration value="Ocupação Ilegal" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="material"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Material</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Alvenaria" />
								<xs:enumeration value="Taipa" />
								<xs:enumeration value="Mista" />
								<xs:enumeration value="Outro" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>
				        <xs:element name="outroMaterial" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Outro material</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>


						<xs:element name="abastecimentoAgua"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Abastecimento Água</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Cagece-Tarifa Social" />
								<xs:enumeration value="Cagece-Sem Tarifa Social" />
								<xs:enumeration value="Ligação Clandestina" />
								<xs:enumeration value="Não tem" />
								<xs:enumeration value="Outro" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>
				        <xs:element name="outroAbastecimento" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Outro Abastecimento</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>


						<xs:element name="abastecimentoEnergia"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Abastecimento de Energia</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Coelce-Tarifa Social" />
								<xs:enumeration value="Coelce- Sem Tarifa Social" />
								<xs:enumeration value="Ligação Clandestina" />
								<xs:enumeration value="Não tem" />
								<xs:enumeration value="Outro" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>
				        <xs:element name="outroAbastecimentoEnergia" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Outro Abastecimento Energia</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>


						<xs:element name="tratamentoAgua"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Tratamento Água</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Filtrada" />
								<xs:enumeration value="Fervida" />
								<xs:enumeration value="Clorada" />
								<xs:enumeration value="Nenhum" />
								<xs:enumeration value="Outro" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>
				        <xs:element name="outroTratamentoAgua" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Outro Tratamento</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>


